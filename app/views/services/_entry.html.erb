

<tr id="entry-<%= object.id %>">
	<td>
		<%= "#{object.name}" %>
	</td>
	<td>
		<%= object.service_category.name %>
	</td>
	
  
	
	<td>
		 <%= print_money( object.recommended_selling_price ) %>
	</td>
	
	<td>
		<%= link_to service_url(object.id ), :class =>"delete_entry", :style => "cursor:pointer; text-decoration:none;" do %>
			<i class='icon-remove'></i>Delete
		<% end %> 
	</td>

	<td>
		<%= link_to edit_service_url(object.id), 
			:class =>"edit_entry modal-toggler", :style => "cursor:pointer; text-decoration:none;" do %>
			<i class='icon-edit'></i>Edit
		<% end %>
	</td>
	
</tr>


