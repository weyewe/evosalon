 
<tr id="entry-<%= object.id %>">
	<td>
		<%= "#{object.name}" %>
	</td>
	<td>
		<ul>
			<li>Telpon: <%= object.phone %></li>
			<li>HP: <%= object.mobile %></li>
			<li>BBM Pin: <%= object.bbm_pin %></li>
			<li>Email: <%= object.email %></li>
		</ul> 
	</td>



	<td>
		 <%= object.address %>
	</td>

	<td>
		<%= link_to employee_url(object.id ), :class =>"delete_entry", :style => "cursor:pointer; text-decoration:none;" do %>
			<i class='icon-remove'></i>Delete
		<% end %> 
	</td>

	<td>
		<%= link_to edit_employee_url(object.id), 
			:class =>"edit_entry modal-toggler", :style => "cursor:pointer; text-decoration:none;" do %>
			<i class='icon-edit'></i>Edit
		<% end %>
	</td>

</tr> 